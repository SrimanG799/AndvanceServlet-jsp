<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>Login</servlet-name>
		<display-name>Login</display-name>
		<description></description>
		<servlet-class>com.requestdisp.Login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Welcome</servlet-name>
		<display-name>Welcome</display-name>
		<description></description>
		<servlet-class>com.well.Welcome</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogOut</servlet-name>
		<display-name>LogOut</display-name>
		<description></description>
		<servlet-class>com.logg.LogOut</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>PageRedirect</servlet-name>
		<display-name>PageRedirect</display-name>
		<description></description>
		<servlet-class>com.pageRedair.PageRedirect</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Browzer</servlet-name>
		<display-name>Browzer</display-name>
		<description></description>
		<servlet-class>com.pageRedair.Browzer</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReadConfig</servlet-name>
		<display-name>ReadConfig</display-name>

		<description></description>
		<servlet-class>com.config.ReadConfig</servlet-class>
		<init-param>
			<param-name>user</param-name>
			<param-value>sriman</param-value>
		</init-param>
		<init-param>
			<param-name>password</param-name>
			<param-value>sri@799</param-value>
		</init-param>
	</servlet>
	<servlet>
		<servlet-name>ContextServ</servlet-name>
		<display-name>ContextServ</display-name>
		<description></description>
		<servlet-class>com.contex.ContextServ</servlet-class>
		
		
	</servlet>
	<servlet-mapping>
		<servlet-name>Login</servlet-name>
		<url-pattern>/log</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Welcome</servlet-name>
		<url-pattern>/Welcome</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogOut</servlet-name>
		<url-pattern>/LogOut</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>PageRedirect</servlet-name>
		<url-pattern>/PageRedirect</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Browzer</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReadConfig</servlet-name>
		<url-pattern>/ReadConfig</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ContextServ</servlet-name>
		<url-pattern>/contex</url-pattern>
	</servlet-mapping>
	<context-param>
        <param-name>City</param-name>
        <param-value>Hyderabad</param-value>
    </context-param>
    <context-param>
        <param-name>Name</param-name>
        <param-value>Sriman</param-value>
    </context-param>
</web-app>
